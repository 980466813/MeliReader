<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <corners android:bottomLeftRadius="@dimen/dp_12" android:bottomRightRadius="@dimen/dp_12" />
            <solid android:color="@color/colorPressGray" />
            <stroke android:width="2dp" android:color="@color/colorPressGray" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:bottomLeftRadius="@dimen/dp_12" android:bottomRightRadius="@dimen/dp_12" />
            <solid android:color="@android:color/transparent" />
            <stroke android:width="2dp" android:color="@color/md_white" />
        </shape>
    </item>
</selector>